{
    "auth": {
        "users": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "usersCustomMessage",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ]
        }
    },
    "api": {
        "content": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "authConfig": {
                    "additionalAuthenticationProviders": [
                        {
                            "authenticationType": "AWS_IAM"
                        }
                    ],
                    "defaultAuthentication": {
                        "authenticationType": "AMAZON_COGNITO_USER_POOLS",
                        "userPoolConfig": {
                            "userPoolId": "authusers"
                        }
                    }
                }
            }
        }
    },
    "storage": {
        "originals": {
            "service": "S3",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "triggerRouter",
                    "attributes": [
                        "Name",
                        "Arn",
                        "LambdaExecutionRole"
                    ]
                }
            ]
        }
    },
    "hosting": {
        "S3AndCloudFront": {
            "service": "S3AndCloudFront",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "usersCustomMessage": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "exifReader": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "originals",
                    "attributes": [
                        "BucketName"
                    ]
                }
            ]
        },
        "triggerRouter": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "buildTree": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "content",
                    "attributes": [
                        "GraphQLAPIIdOutput",
                        "GraphQLAPIEndpointOutput"
                    ]
                },
                {
                    "category": "hosting",
                    "resourceName": "S3AndCloudFront",
                    "attributes": [
                        "HostingBucketName"
                    ]
                }
            ]
        },
        "buildAlbum": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "content",
                    "attributes": [
                        "GraphQLAPIIdOutput",
                        "GraphQLAPIEndpointOutput"
                    ]
                },
                {
                    "category": "hosting",
                    "resourceName": "S3AndCloudFront",
                    "attributes": [
                        "HostingBucketName"
                    ]
                }
            ]
        },
        "runBuild": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "buildTree",
                    "attributes": [
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "buildAlbum",
                    "attributes": [
                        "Name"
                    ]
                }
            ]
        }
    }
}